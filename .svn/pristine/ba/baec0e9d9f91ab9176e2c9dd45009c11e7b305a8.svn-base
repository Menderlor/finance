<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:imagecontrol="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/zl_back"
    android:orientation="vertical"
    tools:context=".SettingActivity" >

    <RelativeLayout style="@style/style_header_layout" >

        <ImageView
            android:id="@+id/imageViewCancel"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_marginLeft="5dip"
            android:padding="5dip"
            android:src="@drawable/back"
            android:visibility="gone" />

        <TextView
            android:id="@+id/textViewTitle"
            style="@style/title_common"
            android:text="设置" />

        <ImageView
            android:id="@+id/imageViewDone"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:layout_marginRight="5dip"
            android:padding="5dip"
            android:src="@drawable/check"
            android:visibility="gone" />
    </RelativeLayout>

    <View
        android:layout_width="fill_parent"
        android:layout_height="4px"
        android:background="@color/lightgray" />

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="wrap_content" >

        <LinearLayout
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="15dp"
            android:orientation="vertical" >

            <LinearLayout
                android:id="@+id/ll_avatar_setting_title"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:clickable="true" >

                <RelativeLayout
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content" >

                    <com.changhui.control.RoundImageView
                        android:id="@+id/imageViewCamera1"
                        android:layout_width="80dp"
                        android:layout_height="80dp"
                        android:layout_centerHorizontal="true"
                        android:src="@drawable/camera"
                        imagecontrol:border_inside_color="#999999"
                        imagecontrol:border_thickness="1dp" />

                    <TextView
                        android:id="@+id/tv_userinfo_setting_company"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_below="@+id/imageViewCamera1"
                        android:layout_marginTop="5dp"
                        android:gravity="center_horizontal|top"
                        android:text="json"
                        android:textColor="#565656"
                        android:textSize="16sp" />

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="40dp"
                        android:layout_below="@id/tv_userinfo_setting_company"
                        android:layout_marginTop="10dp"
                        android:background="#ffffff"
                        android:orientation="horizontal" >

                        <TextView
                            android:id="@+id/tv_useinfo_setting_user"
                            android:layout_width="match_parent"
                            android:layout_height="40dp"
                            android:layout_weight="1"
                            android:gravity="center"
                            android:text="jack"
                            android:textColor="#565656"
                            android:textSize="16sp" />

                        <View
                            android:layout_width="1dp"
                            android:layout_height="30dp"
                            android:visibility="gone"
                            android:layout_gravity="center_vertical"
                            android:background="#999999" />

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="45dp"
                            android:layout_weight="1"
                            android:visibility="gone"
                            android:orientation="horizontal" >

                            <ImageView
                                android:layout_width="match_parent"
                                android:layout_height="30dp"
                                android:layout_gravity="center"
                                android:layout_marginLeft="10dp"
                                android:layout_weight="1"
                                android:src="@drawable/diamondl" />

                            <TextView
                                android:id="@+id/dinamol_zmy_num"
                                android:layout_width="match_parent"
                                android:layout_height="45dp"
                                android:layout_marginRight="10dp"
                                android:layout_weight="1"
                                android:gravity="center"
                                android:text="--"
                                android:textColor="#565656"
                                android:textSize="16sp" />
                        </LinearLayout>

                        <View
                            android:layout_width="1dp"
                            android:layout_height="30dp"
                            android:visibility="gone"
                            android:layout_gravity="center_vertical"
                            android:background="#999999" />

                        <TextView
                            android:id="@+id/zmy_setting_rank"
                            android:layout_width="match_parent"
                            android:layout_height="40dp"
                            android:layout_weight="1"
                            android:gravity="center"
                            android:visibility="gone"
                            android:text="--"
                            android:textColor="#565656"
                            android:textSize="16sp" />
                    </LinearLayout>
                </RelativeLayout>
            </LinearLayout>

            <LinearLayout
                android:id="@+id/ll_avatar_setting"
                style="@style/task_style_ll_info"
                android:layout_height="50dp"
                android:layout_marginBottom="15dp"
                android:layout_marginTop="20dp"
                android:background="@drawable/selector_item_layout"
                android:clickable="true"
                android:paddingLeft="20dp"
                android:paddingRight="10dp" >

                <TextView
                    android:id="@+id/tv_avatar_setting"
                    style="@style/text_style_info"
                    android:layout_weight="3"
                    android:focusable="false"
                    android:text="当前版本"
                    android:textColor="#565656"
                    android:textSize="16sp" />

                <TextView
                    android:id="@+id/tv_version_setting"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:focusable="false"
                    android:text="V 1.0"
                    android:textColor="#2DA8FF"
                    android:textSize="16sp" />
            </LinearLayout>

            <LinearLayout
                android:id="@+id/tixing_set"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@drawable/selector_item_layout"
                android:clickable="true"
                android:focusable="true"
                android:orientation="vertical"
                android:visibility="gone" >

                <LinearLayout
                    style="@style/task_style_ll_info"
                    android:layout_height="50dp"
                    android:paddingLeft="20dp"
                    android:paddingRight="10dp" >

                    <TextView
                        style="@style/text_style_info"
                        android:layout_weight="3"
                        android:text="通知设置"
                        android:textColor="#565656"
                        android:textSize="16sp" />

                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center_vertical"
                        android:src="@drawable/notice_icon01" />
                </LinearLayout>
            </LinearLayout>

            <LinearLayout
                android:id="@+id/ll_invite_download"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@drawable/selector_item_layout"
                android:clickable="true"
                android:orientation="vertical"
                android:visibility="gone" >

                <View style="@style/style_line_horizontal" />

                <LinearLayout
                    style="@style/task_style_ll_info"
                    android:layout_height="50dp"
                    android:paddingLeft="20dp"
                    android:paddingRight="10dp" >

                    <TextView
                        style="@style/text_style_info"
                        android:layout_weight="3"
                        android:focusable="false"
                        android:text="邀请下载"
                        android:textColor="#565656"
                        android:textSize="16sp" />

                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center_vertical"
                        android:src="@drawable/notice_icon01" />
                </LinearLayout>
            </LinearLayout>

            <LinearLayout
                android:id="@+id/diamond_list"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@drawable/selector_item_layout"
                android:clickable="true"
                android:orientation="vertical"
                android:visibility="gone" >

                <View style="@style/style_line_horizontal" />

                <LinearLayout
                    style="@style/task_style_ll_info"
                    android:layout_height="50dp"
                    android:paddingLeft="20dp"
                    android:paddingRight="10dp" >

                    <TextView
                        style="@style/text_style_info"
                        android:layout_height="match_parent"
                        android:layout_weight="3"
                        android:focusable="false"
                        android:gravity="center_vertical"
                        android:text="钻石排行榜"
                        android:textColor="#565656"
                        android:textSize="16sp" />

                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center_vertical"
                        android:src="@drawable/notice_icon01" />
                </LinearLayout>
            </LinearLayout>
            
            <View style="@style/style_line_horizontal" />
            
            <LinearLayout
                android:id="@+id/ll_set_pwd"
                style="@style/task_style_ll_info"
                android:layout_height="50dp"
                android:background="@drawable/selector_item_layout"
                android:focusable="true"
                android:paddingLeft="20dp"
                android:paddingRight="10dp" >
                <TextView
                    style="@style/text_style_info"
                    android:layout_weight="3"
                    android:text="修改密码"
                    android:textColor="#565656"
                    android:textSize="16sp" />

                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_vertical"
                    android:src="@drawable/notice_icon01" />
            </LinearLayout>

            <LinearLayout
                android:id="@+id/ll_share_setting"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@drawable/selector_item_layout"
                android:clickable="true"
                android:orientation="vertical"
                android:visibility="gone" >

                <View style="@style/style_line_horizontal" />

                <LinearLayout
                    style="@style/task_style_ll_info"
                    android:layout_height="50dp"
                    android:paddingLeft="20dp"
                    android:paddingRight="10dp" >

                    <TextView
                        style="@style/text_style_info"
                        android:layout_weight="3"
                        android:text="好用就分享下"
                        android:textColor="#565656"
                        android:textSize="16sp" />

                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center_vertical"
                        android:src="@drawable/notice_icon01" />
                </LinearLayout>
            </LinearLayout>

            <LinearLayout
                android:id="@+id/ll_check_ad"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@drawable/selector_item_layout"
                android:clickable="true"
                android:orientation="vertical"
                android:visibility="gone" >

                <View style="@style/style_line_horizontal" />

                <LinearLayout
                    style="@style/task_style_ll_info"
                    android:layout_height="50dp"
                    android:paddingLeft="20dp"
                    android:paddingRight="10dp" >

                    <TextView
                        style="@style/text_style_info"
                        android:layout_weight="3"
                        android:text="问题反馈"
                        android:textColor="#565656"
                        android:textSize="16sp" />

                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center_vertical"
                        android:src="@drawable/notice_icon01" />
                </LinearLayout>
            </LinearLayout>

            <LinearLayout
                android:id="@+id/ll_url_ad"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:clickable="true"
                android:padding="30dp" >

                <Button
                    android:id="@+id/btn_quit_setting"
                    android:layout_width="match_parent"
                    android:layout_height="40dp"
                    android:background="@drawable/esc"
                    android:text="           退 出 登 录          "
                    android:textColor="@color/white"
                    android:textSize="16sp" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="100dp"
                android:visibility="gone" >
            </LinearLayout>
        </LinearLayout>
    </ScrollView>

</LinearLayout>