<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:android_custom="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#F5F5F5"
    android:orientation="vertical" >

    <RelativeLayout
        android:id="@+id/title"
        android:layout_width="match_parent"
        android:layout_height="45dp"
        android:background="#0099ff" >

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerInParent="true"
            android:text="任务统计"
            android:textColor="#ffffff"
            style="@style/title_common" />

        <ImageButton
            android:id="@+id/taskmore_back"
            android:layout_width="45dp"
            android:layout_height="45dp"
            android:background="@null"
            android:src="@drawable/back" />
    </RelativeLayout>

    <LinearLayout
        android:id="@+id/task_more_selectdate"
        android:layout_width="match_parent"
        android:layout_height="30dp"
        android:layout_marginLeft="20dp"
        android:layout_marginRight="20dp"
        android:layout_marginTop="15dp"
        android:background="@drawable/task_more_seletdate"
        android:orientation="horizontal"
        android:padding="0.5dp" >

        <TextView
            android:id="@+id/taskmore_week"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:gravity="center"
            android:text="本周"
            android:textColor="#0099ff" />

        <TextView
            android:id="@+id/taskmore_mounth"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:gravity="center"
            android:text="本月"
            android:textColor="#0099ff" />

        <TextView
            android:id="@+id/taskmore_date"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:gravity="center"
            android:padding="1dp"
            android:text="时间段"
            android:textColor="#0099ff" />
    </LinearLayout>

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="5dp"
        android:padding="20dp" >

        <TextView
            android:id="@+id/taskmore_info"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="本月任务详情"
            android:textColor="#0099ff"
            android:textSize="17sp" />

        <RelativeLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:layout_below="@+id/taskmore_info"
            android:layout_marginTop="10dp" >

            <TextView
                android:id="@+id/taskmore_color"
                android:layout_width="25dp"
                android:layout_height="15dp"
                android:layout_centerVertical="true"
                android:background="#93D492" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_marginLeft="10dp"
                android:layout_toRightOf="@id/taskmore_color"
                android:text="完成"
                android:textColor="#93D492"
                android:textSize="15dp" />
        </RelativeLayout>
    </RelativeLayout>

    <LinearLayout
        android:id="@+id/progress"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="5dp"
        android:layout_marginRight="5dp"
        android:layout_marginTop="5dp"
        android:orientation="horizontal" >

        <RelativeLayout
            android:id="@+id/all_relative"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:paddingBottom="5dp"
            android:paddingTop="5dp" >

            <com.changhui.control.RoundProgressBar
                android:id="@+id/taskmore_all"
                android:layout_width="90dp"
                android:layout_height="90dp"
                android:layout_centerHorizontal="true"
                android_custom:roundColor="#E7DEDF"
                android_custom:roundProgressColor="#93D492"
                android_custom:roundWidth="8dp"
                android_custom:textSize="18sp" />

            <TextView
                android:id="@+id/all_task_zmy"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@id/taskmore_all"
                android:layout_centerHorizontal="true"
                android:layout_marginTop="10dp"
                android:text="全部任务"
                android:textColor="#969696" />
        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/my_relative"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:paddingBottom="5dp"
            android:paddingTop="5dp" >

            <com.changhui.control.RoundProgressBar
                android:id="@+id/moretask_my"
                android:layout_width="90dp"
                android:layout_height="90dp"
                android:layout_centerHorizontal="true"
                android_custom:roundColor="#E7DEDF"
                android_custom:roundProgressColor="#93D492"
                android_custom:roundWidth="8dp"
                android_custom:textSize="18sp" />

            <TextView
                android:id="@+id/task_my_zmy"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/moretask_my"
                android:layout_centerHorizontal="true"
                android:layout_marginTop="10dp"
                android:text="我的任务"
                android:textColor="#969696" />
        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/inssed_relative"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:paddingBottom="5dp"
            android:paddingTop="5dp" >

            <com.changhui.control.RoundProgressBar
                android:id="@+id/moretask_issued"
                android:layout_width="90dp"
                android:layout_height="90dp"
                android:layout_centerHorizontal="true"
                android_custom:roundColor="#E7DEDF"
                android_custom:roundProgressColor="#93D492"
                android_custom:roundWidth="8dp"
                android_custom:textSize="18sp" />

            <TextView
                android:id="@+id/my_inssed"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@id/moretask_issued"
                android:layout_centerHorizontal="true"
                android:layout_marginTop="10dp"
                android:text="我下达的任务"
                android:textColor="#969696" />
        </RelativeLayout>
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp"
        android:background="#ffffff"
        android:orientation="vertical" >

        <RelativeLayout
            android:id="@+id/layout_taskmore_ok"
            android:layout_width="match_parent"
            android:layout_height="50dp" >

            <TextView
                android:id="@+id/taskmore_title_ok"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_marginLeft="20dp"
                android:text="bemyue:"
                android:textColor="#383838"
                android:textSize="16sp" />

            <TextView
                android:id="@+id/taskmore_title_ok_num"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_marginLeft="5dp"
                android:layout_toRightOf="@id/taskmore_title_ok"
                android:text="30"
                android:textColor="#32BD31"
                android:textSize="16sp" />

            <ImageButton
                android:id="@+id/taskmore_inok"
                android:layout_width="50dp"
                android:layout_height="50dp"
                android:layout_alignParentRight="true"
                android:background="@null"
                android:src="@drawable/notice_icon01" />
        </RelativeLayout>

        <TextView
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:layout_marginLeft="10dp"
            android:layout_marginRight="10dp"
            android:background="#F5F5F5" />

        <RelativeLayout
            android:id="@+id/layout_taskmore_no"
            android:layout_width="match_parent"
            android:layout_height="50dp" >

            <TextView
                android:id="@+id/taskmore_title_no"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_marginLeft="20dp"
                android:text="bemyue:"
                android:textColor="#383838"
                android:textSize="16sp" />

            <TextView
                android:id="@+id/taskmore_title_no_num"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_marginLeft="5dp"
                android:layout_toRightOf="@id/taskmore_title_no"
                android:text="30"
                android:textColor="#878787"
                android:textSize="16sp" />

            <ImageButton
                android:id="@+id/taskmore_inno"
                android:layout_width="50dp"
                android:layout_height="50dp"
                android:layout_alignParentRight="true"
                android:background="@null"
                android:src="@drawable/notice_icon01" />
        </RelativeLayout>

        <TextView
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:layout_marginLeft="10dp"
            android:layout_marginRight="10dp"
            android:background="#F5F5F5" />

        <RelativeLayout
            android:id="@+id/layout_taskmore_delay"
            android:layout_width="match_parent"
            android:layout_height="50dp" >

            <TextView
                android:id="@+id/taskmore_title_delay"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_marginLeft="20dp"
                android:text="所有延期任务："
                android:textColor="#383838"
                android:textSize="16sp" />

            <TextView
                android:id="@+id/taskmore_title_delay_num"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_marginLeft="5dp"
                android:layout_toRightOf="@id/taskmore_title_delay"
                android:text="30"
                android:textColor="#D62221"
                android:textSize="16sp" />

            <ImageButton
                android:id="@+id/taskmore_in3"
                android:layout_width="50dp"
                android:layout_height="50dp"
                android:layout_alignParentRight="true"
                android:background="@null"
                android:src="@drawable/notice_icon01" />
        </RelativeLayout>
        
        
        <TextView
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:layout_marginLeft="10dp"
            android:layout_marginRight="10dp"
            android:background="#F5F5F5" />
        <RelativeLayout
            android:id="@+id/layout_taskmore_unread"
            android:layout_width="match_parent"
            android:layout_height="50dp" >

            <TextView
                android:id="@+id/taskmore_title_unread"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_marginLeft="20dp"
                android:text="所有未读任务："
                android:textColor="#383838"
                android:textSize="16sp" />

            <TextView
                android:id="@+id/taskmore_title_unread_num"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_marginLeft="5dp"
                android:layout_toRightOf="@id/taskmore_title_unread"
                android:text="0"
                android:textColor="#D62221"
                android:textSize="16sp" />

            <ImageButton
                android:id="@+id/taskmore_in4"
                android:layout_width="50dp"
                android:layout_height="50dp"
                android:layout_alignParentRight="true"
                android:background="@null"
                android:src="@drawable/notice_icon01" />
        </RelativeLayout>
        
        <TextView
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:layout_marginLeft="10dp"
            android:layout_marginRight="10dp"
            android:background="#F5F5F5" />
        <RelativeLayout
            android:id="@+id/layout_taskmore_all"
            android:layout_width="match_parent"
            android:layout_height="50dp" >

            <TextView
                android:id="@+id/taskmore_title_all"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_marginLeft="20dp"
                android:text="所有任务："
                android:textColor="#383838"
                android:textSize="16sp" />

            <TextView
                android:id="@+id/taskmore_title_all_num"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_marginLeft="5dp"
                android:layout_toRightOf="@+id/taskmore_title_all"
                android:text="0"
                android:textColor="#D62221"
                android:textSize="16sp" />

            <ImageButton
                android:id="@+id/taskmore_in5"
                android:layout_width="50dp"
                android:layout_height="50dp"
                android:layout_alignParentRight="true"
                android:background="@null"
                android:src="@drawable/notice_icon01" />
        </RelativeLayout>
    </LinearLayout>

</LinearLayout>