<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:background="@color/lightgray" >

        <RelativeLayout
            android:id="@+id/rl_calendar_last_week"
            android:layout_width="70dp"
            android:layout_height="50dp"
            android:layout_centerVertical="true"
            android:layout_toLeftOf="@+id/tv_this_week"
            android:clickable="true"
            android:focusable="true" >

            <TextView
                android:layout_width="9dp"
                android:layout_height="14dp"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:layout_marginRight="40dp"
                android:background="@drawable/calendar_last_unfocused" />
        </RelativeLayout>

        <TextView
            android:id="@+id/tv_this_week"
            android:layout_width="120dp"
            android:layout_height="match_parent"
            android:layout_centerInParent="true"
            android:clickable="true"
            android:gravity="center"
            android:text="查看本周"
            android:textColor="#aa564b4b"
            android:textSize="18sp" />

        <RelativeLayout
            android:id="@+id/rl_calendar_next_week"
            android:layout_width="70dp"
            android:layout_height="50dp"
            android:layout_centerVertical="true"
            android:layout_toRightOf="@+id/tv_this_week"
            android:clickable="true"
            android:focusable="true" >

            <TextView
                android:layout_width="9dp"
                android:layout_height="14dp"
                android:layout_alignParentLeft="true"
                android:layout_centerVertical="true"
                android:layout_marginLeft="40dp"
                android:background="@drawable/calendar_next_unfocused" />
        </RelativeLayout>
    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/rl_pbar_week_calendar"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@drawable/business_hall_selector_normall" >

        <com.cedarhd.control.MyProgressBar
            android:id="@+id/pbar_week_calendar"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerHorizontal="true" ></com.cedarhd.control.MyProgressBar>
    </RelativeLayout>

    <ScrollView
        android:id="@+id/scolllayout"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:background="@drawable/business_hall_selector_normall"
        android:fillViewport="true" >

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical" >

