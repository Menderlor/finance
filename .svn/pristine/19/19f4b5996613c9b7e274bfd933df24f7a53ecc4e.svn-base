<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:boeryun="http://schemas.android.com/apk/res-auto"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:background="@color/bg_list"
    android:orientation="vertical" >

    <com.changhui.control.BoeryunHeaderView
        android:id="@+id/header_rad_report_list"
        style="@style/style_header_layout"
        boeryun:isVisbleAddIco="true"
        boeryun:isVisbleBackIco="true"
        boeryun:isVisbleFilterIco="false"
        boeryun:isVisbleSaveIco="false"
        boeryun:titleText="上报列表" />


    <LinearLayout 
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="1">
        <include layout="@layout/merge_listview_old_style"/>
    </LinearLayout>

</LinearLayout>
