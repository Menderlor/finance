<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:boeryun="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white"
    android:orientation="vertical" >

    <com.changhui.control.BoeryunHeaderView
        android:id="@+id/header_ch_bespoke_info"
        style="@style/style_header_layout"
        boeryun:isVisbleAddIco="true"
        boeryun:isVisbleBackIco="true"
        boeryun:isVisbleFilterIco="false"
        boeryun:isVisbleSaveIco="true"
        boeryun:titleText="预约详情" />

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent" >

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical" >

            <LinearLayout style="@style/style_changhui_ll_info" >

                <TextView
                    style="@style/style_changhui_text_left_info"
                    android:text="基金名称" />

                <TextView
                    android:id="@+id/tv_product_ch_bespoke_info"
                    style="@style/style_changhui_text_right_info" />
            </LinearLayout>
            <View style="@style/style_line_horizontal" />

            <LinearLayout
                android:id="@+id/ll_rengou_ch_bespoke_info"
                style="@style/style_changhui_readonly_ll_info" >

                <TextView
                    style="@style/style_changhui_text_left_info"
                    android:text="认购类型" />

                <TextView
                    android:id="@+id/tv_rengou_ch_bespoke_info"
                    style="@style/style_changhui_text_right_info" />
            </LinearLayout>

            <View style="@style/style_line_horizontal" />

            <LinearLayout style="@style/style_changhui_ll_info" >

                <TextView
                    style="@style/style_changhui_text_left_info"
                    android:text="客户姓名" />

                <TextView
                    android:id="@+id/tv_client_ch_bespoke_info"
                    style="@style/style_changhui_text_right_info" />
            </LinearLayout>

            <View style="@style/style_line_horizontal" />

            <LinearLayout style="@style/style_changhui_ll_info" >

                <TextView
                    style="@style/style_changhui_text_left_info"
                    android:text="证件类型" />

                <com.changhui.control.BoeryunDictSelectView
                    android:id="@+id/tv_card_type_ch_bespoke_info"
                    style="@style/style_changhui_text_right_info"
                    boeryun:dictName="客户_证件类别" />
            </LinearLayout>

            <View style="@style/style_line_horizontal" />

            <LinearLayout style="@style/style_changhui_ll_info" >

                <TextView
                    style="@style/style_changhui_text_left_info"
                    android:text="证件号码" />

                <EditText
                    android:id="@+id/et_card_no_ch_bespoke_info"
                    style="@style/style_changhui_text_right_info" />
            </LinearLayout>

            <View style="@style/style_line_horizontal" />

            <LinearLayout style="@style/style_changhui_ll_info" >

                <TextView
                    style="@style/style_changhui_text_left_info"
                    android:text="手机" />

                <EditText
                    android:id="@+id/tv_phone_ch_bespoke_info"
                    style="@style/style_changhui_text_right_info"
                    android:inputType="phone" />
            </LinearLayout>
            
            <View style="@style/style_line_horizontal" />

            <LinearLayout
                android:id="@+id/ll_zuoji_ch_bespoke_info"
                style="@style/style_changhui_ll_info" >

                <TextView
                    style="@style/style_changhui_text_left_info"
                    android:text="座机" />

                <EditText
                    android:id="@+id/et_zuoji_ch_bespoke_info"
                    style="@style/style_changhui_text_right_info" 
                    android:inputType="phone" />
            </LinearLayout>

            <View style="@style/style_line_horizontal" />

            <LinearLayout
                android:id="@+id/ll_email_ch_bespoke_info"
                style="@style/style_changhui_ll_info" >

                <TextView
                    style="@style/style_changhui_text_left_info"
                    android:text="邮箱" />

                <EditText
                    android:id="@+id/et_email_ch_bespoke_info"
                    style="@style/style_changhui_text_right_info" />
            </LinearLayout>

            <View style="@style/style_line_horizontal" />

            <LinearLayout style="@style/style_changhui_ll_info" >

                <TextView
                    style="@style/style_changhui_text_left_info"
                    android:text="预约金额(元)" />

                <EditText
                    android:id="@+id/tv_bespoke_total_ch_bespoke_info"
                    style="@style/style_changhui_text_right_info"
                    android:inputType="number" />
            </LinearLayout>

            <View style="@style/style_line_horizontal" />

            <LinearLayout
                android:id="@+id/ll_total_big_ch_bespoke_info"
                style="@style/style_changhui_readonly_ll_info" >

                <TextView
                    style="@style/style_changhui_text_left_info"
                    android:text="预约金额大写" />

                <TextView
                    android:id="@+id/tv_total_big_ch_bespoke_info"
                    style="@style/style_changhui_text_right_info" />
            </LinearLayout>
            
            <View style="@style/style_line_horizontal" />

            <LinearLayout style="@style/style_changhui_ll_info" >

                <TextView
                    style="@style/style_changhui_text_left_info"
                    android:text="预约打款时间" />

                <com.changhui.control.BoeryunDateSelectView
                    android:id="@+id/tv_pay_time_ch_bespoke_info"
                    style="@style/style_changhui_text_right_info"
                    boeryun:defaultValue="true"
                    boeryun:isShowDate="false"
                    boeryun:isShowTime="true" />
            </LinearLayout>

            <View style="@style/style_line_horizontal" />

            <LinearLayout style="@style/style_changhui_ll_info" >

                <TextView
                    style="@style/style_changhui_text_left_info"
                    android:text="投资期限(月)" />

                <EditText
                    android:id="@+id/tv_deadline_ch_bespoke_info"
                    style="@style/style_changhui_text_right_info"
                    android:inputType="number" />
            </LinearLayout>

            <View style="@style/style_line_horizontal" />

            <LinearLayout style="@style/style_changhui_ll_info" >
                <TextView
                    style="@style/style_changhui_text_left_info"
                    android:text="产品分类" />

                <EditText
                    android:id="@+id/et_category_ch_bespoke_info"
                    style="@style/style_changhui_text_right_info" />
            </LinearLayout>

            <View style="@style/style_line_horizontal" />

            <LinearLayout
                android:id="@+id/ll_nianhuashouyi_ch_bespoke_info"
                style="@style/style_changhui_ll_info" >

                <TextView
                    style="@style/style_changhui_text_left_info"
                    android:text="业绩比较基准" />
                <!-- 预期年化收益率 -->

                <EditText
                    android:id="@+id/et_nianhuashouyi_ch_bespoke_info"
                    style="@style/style_changhui_text_right_info" 
                    android:inputType="number" />
            </LinearLayout>
            
            <View style="@style/style_line_horizontal" />

            <LinearLayout style="@style/style_changhui_ll_info" >

                <TextView
                    style="@style/style_changhui_text_left_info"
                    android:text="理财经理" />

                <TextView
                    android:id="@+id/tv_client_mannager_ch_bespoke_info"
                    style="@style/style_changhui_text_right_info" />
            </LinearLayout>
            
            <View style="@style/style_line_horizontal" />

            <LinearLayout
                android:id="@+id/ll_dept_ch_bespoke_info"
                style="@style/style_changhui_readonly_ll_info" >

                <TextView
                    style="@style/style_changhui_text_left_info"
                    android:text="所属团队" />

                <TextView
                    android:id="@+id/tv_dept_ch_bespoke_info"
                    style="@style/style_changhui_text_right_info" />
            </LinearLayout>
            
            <View style="@style/style_line_horizontal" />

            <LinearLayout
                android:id="@+id/ll_company_ch_bespoke_info"
                style="@style/style_changhui_readonly_ll_info" >

                <TextView
                    style="@style/style_changhui_text_left_info"
                    android:text="所属分公司" />

                <TextView
                    android:id="@+id/tv_company_bespoke_info"
                    style="@style/style_changhui_text_right_info" />
            </LinearLayout>

       

            <!-- 2012-12-22新添加字段 -->

            

            <View style="@style/style_line_horizontal" />

            <LinearLayout
                android:id="@+id/ll_deadline_ch_bespoke_info"
                style="@style/style_changhui_readonly_ll_info" >

                <TextView
                    style="@style/style_changhui_text_left_info"
                    android:text="预约到期日期" />

                <TextView
                    android:id="@+id/tv_outtime_ch_bespoke_info"
                    style="@style/style_changhui_text_right_info" />
            </LinearLayout>
            

            <View style="@style/style_line_horizontal" />

            <LinearLayout
                android:id="@+id/ll_kehujinglishouji_ch_bespoke_info"
                style="@style/style_changhui_ll_info" >

                <TextView
                    style="@style/style_changhui_text_left_info"
                    android:text="客户经理手机" />

                <EditText
                    android:id="@+id/et_kehujinglishouji_ch_bespoke_info"
                    style="@style/style_changhui_text_right_info" />
            </LinearLayout>

            <View style="@style/style_line_horizontal" />

            <LinearLayout
                android:id="@+id/ll_kehujingliyouxiang_ch_bespoke_info"
                style="@style/style_changhui_ll_info" >

                <TextView
                    style="@style/style_changhui_text_left_info"
                    android:text="客户经理邮箱" />

                <EditText
                    android:id="@+id/et_kehujingli_email_ch_bespoke_info"
                    style="@style/style_changhui_text_right_info" />
            </LinearLayout>

            

            <View style="@style/style_line_horizontal" />

            <LinearLayout
                style="@style/style_changhui_ll_info"
                android:gravity="center"
                android:padding="10dp" >

                <TextView
                    android:id="@+id/tv_submit_ch_bespoke_info"
                    style="@style/style_button_blue"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="提 交"
                    android:visibility="gone" />
            </LinearLayout>
        </LinearLayout>
    </ScrollView>

</LinearLayout>