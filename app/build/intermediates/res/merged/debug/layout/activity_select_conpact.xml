<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical" 
    android:background="@color/white">
    <RelativeLayout style="@style/style_header_layout" >
        <ImageView
            android:id="@+id/iv_back_select_conpact"
            style="@style/style_header_button"
            android:src="@drawable/back" />

        <TextView
            style="@style/title_common"
            android:text="选择合同"/>

    </RelativeLayout>

    <View style="@style/style_line_horizontal"/>
    <ListView 
        android:id="@+id/lv_select_conpact"
        style="@style/style_listview"/>
</LinearLayout>

